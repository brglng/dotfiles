{
    "coc.preferences.formatOnType": false,
    "clangd.arguments": ["--background-index", "--completion-style=detailed", "--cross-file-rename", "--limit-results=10000", "-j=2", "--pch-storage=disk"],
    "clangd.fallbackFlags": ["-std=c++17"],
    // "clangd.trace.server": "verbose",
    // "clangd.trace.file": "clangd-trace.log",
    "diagnostic.errorSign": "",
    "diagnostic.hintSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextLines": 20,
    "diagnostic.warningSign": "",
    "explorer.git.icon.status.added": "",
    "explorer.git.icon.status.copied": "",
    "explorer.git.icon.status.deleted": "-",
    "explorer.git.icon.status.ignored": "",
    "explorer.git.icon.status.mixed": "*",
    "explorer.git.icon.status.modified": "",
    "explorer.git.icon.status.renamed": "",
    "explorer.git.icon.status.unmerged": "",
    "explorer.git.icon.status.unmodified": " ",
    "explorer.git.icon.status.untracked": "",
    "explorer.file.column.indent.indentLine": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappings.global": {
        "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
        "<2-LeftMouse>": ["expandable?", ["expanded?", "collapse", "expand"], "open"]
    },
    "explorer.sources": [
        {
            "name": "file",
            "expand": true
        }
    ],
    "explorer.previewAction.onHover": false,
    "git.addedSign.text": "+",
    "git.addGBlameToVirtualText": false,
    "git.addGBlameToBufferVar": true,
    "git.changedDecorator": " ",
    "git.changedSign.text": "~",
    "git.changeRemovedSign.text": "-",
    "git.conflictedDecorator": " ",
    "git.gitlab.hosts": [
        "gitlab.ximalaya.com"
    ],
    "git.removedSign.text": "\u00a0",
    "git.stagedDecorator": " ",
    "git.topRemovedSign.text": "\u00a0",
    "git.untrackedDecorator": " ",
    "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
    "list.source.grep.defaultOptions": ["--interactive", "--no-sort", "--regex"],
    "python.linting.mypyEnabled": true,
    "python.linting.mypyArgs": [
        "--ignore-missing-imports",
        "--follow-imports=silent",
        "--show-column-numbers"
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": ["--disable=wildcard-import,invalid-name,missing-function-docstring,broad-except,logging-format-interpolation,missing-module-docstring,missing-class-docstring,line-too-long,too-few-public-methods,inconsistent-return-statements,too-many-branches,too-many-locals,too-many-statements,too-many-arguments,too-many-instance-attributes,too-many-return-statements", "--enable=unused-import"],
    "snippets.extends": {
        "cpp": ["c"],
        "javascriptreact": ["javascript"],
        "typescript": ["javascript"]
    },
    "snippets.trace": "verbose",
    "suggest.completionItemKindLabels": {
        "class": "\uf0e8",
        "color": "\ue22b",
        "constant": "\uf8ff",
        "default": "\uf29c",
        "event": "\ufacd",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "field": "\uf93d",
        "file": "\uf471",
        "folder": "\uf115",
        "function": "\uf794",
        "interface": "\ufa52",
        "keyword": "\uf893",
        "method": "\uf6a6",
        "module": "\uf668",
        "operator": "\uf915",
        "property": "\ufab6",
        "reference": "\uf87a",
        "snippet": "\uf64d",
        "struct": "\ufb44",
        "text": "\ue612",
        "typeParameter": "\uf278",
        "unit": "\uf475",
        "value": "\uf8a3",
        "variable": "\uf71b"
    },
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "inlayHint.enable": false,
    "tabnine.priority": 0,
    "java.import.gradle.java.home": "/usr/lib/jvm/java-11-openjdk-amd64"
}
