  </alias>
  <alias>
    <family>Meslo LG L</family>
    <prefer><family>PowerlineSymbols</family></prefer>
  </alias>
  <alias>
    <family>Meslo LG L DZ</family>
    <prefer><family>PowerlineSymbols</family></prefer>
  </alias>
  <alias>
    <family>Meslo LG M</family>
    <prefer><family>PowerlineSymbols</family></prefer>
  </alias>
  <alias>
    <family>Meslo LG M DZ</family>
    <prefer><family>PowerlineSymbols</family></prefer>
  </alias>
  <alias>
    <family>Meslo LG S</family>
