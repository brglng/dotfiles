{
  "diagnostic.virtualText": true,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
        "index": {
          "initialBlacklist": ["build/", "cmake-build-.*/"],
          "onChange": true,
          "trackDependency": 2
        }
      }
    }
  },
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "snippets.trace": "verbose",
  "list.maxHeight": 20,
  "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
  "list.source.grep.defaultOptions": ["--interactive", "--no-sort", "--regex"],
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf668",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf93d",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "diagnostic.warningSign": "\uf071",
  "diagnostic.errorSign": "\ufaac",
  "diagnostic.infoSign": "\uf129",
  "diagnostic.hintSign": "\ufbe6",
  "git.addGlameToVirtualText": true,
  "git.addGlameToBufferVar": true,
  "git.changedDecorator": " *",
  "git.stagedDecorator": " ●",
  "git.untrackedDecorator": " …",
  "git.conflictedDecorator": " x",
  "git.addedSign.text": "█",
  "git.changedSign.text": "█",
  "git.removedSign.text": "█",
  "git.topRemovedSign.text": "█",
  "git.changeRemovedSign.text": "█",
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.icon.enableNerdfont": true,
  "explorer.file.column.indent.indentLine": true,
  "explorer.keyMappings": {
    "<cr>": "expand",
    "<2-LeftMouse>": "expand"
  },
  "python.jediEnabled": false
}
