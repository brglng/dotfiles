{
    "diagnostic.errorSign": "",
    "diagnostic.hintSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.virtualText": true,
    "diagnostic.warningSign": "",
    "explorer.file.column.git.icon.added": "",
    "explorer.file.column.git.icon.copied": "",
    "explorer.file.column.git.icon.deleted": "",
    "explorer.file.column.git.icon.ignored": "",
    "explorer.file.column.git.icon.mixed": "*",
    "explorer.file.column.git.icon.modified": "",
    "explorer.file.column.git.icon.renamed": "",
    "explorer.file.column.git.icon.unmerged": "",
    "explorer.file.column.git.icon.unmodified": " ",
    "explorer.file.column.git.icon.untracked": "",
    "explorer.file.column.indent.indentLine": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappings": {
        "<cr>": ["expandable?", "expandOrCollapse", "open"],
        "<2-LeftMouse>": ["expandable?", "expandOrCollapse", "open"]
    },
    "explorer.sources": [
        {
            "name": "buffer",
            "expand": true
        },
        {
            "name": "file",
            "expand": true
        }
    ],
    "explorer.previewAction.onHover": false,
    "git.addedSign.text": "",
    "git.addGBlameToVirtualText": false,
    "git.addGBlameToBufferVar": true,
    "git.changedDecorator": " ",
    "git.changedSign.text": "",
    "git.changeRemovedSign.text": "",
    "git.conflictedDecorator": " ",
    "git.gitlab.hosts": [
        "gitlab.ximalaya.com"
    ],
    "git.removedSign.text": "",
    "git.stagedDecorator": " ",
    "git.topRemovedSign.text": "",
    "git.untrackedDecorator": " ",
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": ["c", "cpp", "objc", "objcpp"],
            "initializationOptions": {
                "index": {
                    "initialBlacklist": ["build/", "cmake-build-.*/"],
                    "onChange": true,
                    "trackDependency": 2
                }
            },
            "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"]
        }
    },
    "list.autoResize": false,
    "list.maxHeight": 15,
    "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
    "list.source.grep.defaultOptions": ["--interactive", "--no-sort", "--regex"],
    "python.jediEnabled": false,
    "python.linting.mypyEnabled": false,
    "python.linting.pylintEnabled": false,
    "python.linting.pylintArgs": ["--disable=wildcard-import,invalid-name,missing-function-docstring,broad-except,logging-format-interpolation,missing-module-docstring,missing-class-docstring,line-too-long,too-few-public-methods,inconsistent-return-statements,too-many-branches,too-many-locals,too-many-statements,too-many-arguments,too-many-instance-attributes", "--enable=unused-import"],
    "pyright.reportMissintTypeStubs": false,
    "snippets.extends": {
        "cpp": ["c"],
        "javascriptreact": ["javascript"],
        "typescript": ["javascript"]
    },
    "snippets.trace": "verbose",
    "suggest.completionItemKindLabels": {
        "class": "\uf0e8",
        "color": "\ue22b",
        "constant": "\uf8ff",
        "default": "\uf29c",
        "event": "\ufacd",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "field": "\uf93d",
        "file": "\uf471",
        "folder": "\uf115",
        "function": "\uf794",
        "interface": "\ufa52",
        "keyword": "\uf893",
        "method": "\uf6a6",
        "module": "\uf668",
        "operator": "\uf915",
        "property": "\ufab6",
        "reference": "\uf87a",
        "snippet": "\uf64d",
        "struct": "\ufb44",
        "text": "\ue612",
        "typeParameter": "\uf278",
        "unit": "\uf475",
        "value": "\uf8a3",
        "variable": "\uf71b"
    }
}
